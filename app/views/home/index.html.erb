
<%= render "header" %>

<%= render "nav" %>

<h2 class="subtitle"> SalesLoft: Level 1 </h2>

    <table>
        <tr>
            <th>Name</th>
            <th>Email</th>
            <th>Job Title</th>
        </tr>
        <% @first_name.zip(@email_address, @title).each do |name, email, title| %>
        <tr>
            <td><%= name %></td>
            <td style="padding: 0px 30px 0px 30px;"><%= email %></td>
            <td><%= title %></td>
        </tr>
        <% end %>
    </table>

